{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "generate"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "generate": {},
    "format": {},
    "clean": {},
    "clean:all": {},
    "dev": {
      "dependsOn": [
        "generate",
        "server#build"
      ],
      "persistent": true,
      "cache": false
    }
  }
}
